        <activity android:name="sampleProject1.ExampleTextures" android:label="@string/app_name"></activity>
        <activity android:name="sampleProject1.ExampleVerticesVariations" android:label="@string/app_name"></activity>
        <activity android:name="sampleProject1.ExampleRenderType" android:label="@string/app_name"></activity>
        <activity android:name="sampleProject1.ExampleCamera" android:label="@string/app_name"></activity>
        <activity android:name="sampleProject1.ExampleMultipleLights" android:label="@string/app_name"></activity>
        <activity android:name="sampleProject1.ExampleAnimatingVertices" android:label="@string/app_name"></activity>
        <activity android:name="sampleProject1.ExampleSubsetOfFaces" android:label="@string/app_name"></activity>
        <activity android:name="sampleProject1.ExampleAssigningTexturesDynamically" android:label="@string/app_name"></activity>
