        <activity android:name="sampleProject1.ExampleFog" android:label="@string/app_name"></activity>
        <activity android:name="sampleProject1.ExampleTransparentGlSurface" android:label="@string/app_name" android:theme="@android:style/Theme.Translucent.NoTitleBar"></activity>
        <activity android:name="sampleProject1.ExampleLoadObjFile" android:label="@string/app_name"></activity>
        <activity android:name="sampleProject1.ExampleLoadObjFileMultiple"></activity>
        <activity android:name="sampleProject1.ExampleLoad3DSFile"></activity>
